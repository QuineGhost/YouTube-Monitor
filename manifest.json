{
  "name": "Youtube-Monitor",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "youtube monitor",
  "browser_action": {
      "name": "Youtube Monitor"
  },
  "background": {
      "scripts": ["scripts/background.js"]
  },
  "permissions": [
      "tabs"
  ],
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["scripts/contents.js"]
      }
  ]
}
